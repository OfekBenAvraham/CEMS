<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui_controllers.HeadOfDepartmentNavigationStatsController">
   <children>
      <Pane fx:id="mainPane" prefHeight="333.0" prefWidth="397.0" style="-fx-background-color: #FFEEDB;">
         <children>
            <Label layoutX="127.0" layoutY="58.0" prefHeight="25.0" prefWidth="151.0" text="Statistics Navigation">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Button fx:id="btnLecturer" layoutX="110.0" layoutY="108.0" mnemonicParsing="false" onAction="#getLecturerStats" prefHeight="25.0" prefWidth="184.0" style="-fx-background-color: #D3B291;" text="View By Lecturer" textFill="#344955" />
            <Button fx:id="btnCourse" layoutX="110.0" layoutY="148.0" mnemonicParsing="false" onAction="#getCourseStats" prefHeight="25.0" prefWidth="184.0" style="-fx-background-color: #D3B291;" text="View By Course" textFill="#344955" />
            <Button fx:id="backBtn" layoutX="336.0" layoutY="292.0" mnemonicParsing="false" onAction="#getExitButton" style="-fx-background-color: #F86060;" text="Back">
               <font>
                  <Font name="Arial" size="13.0" />
               </font>
            </Button>
            <ImageView fitHeight="63.0" fitWidth="54.0" layoutX="339.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@/images/cemsLogo.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="40.0" fitWidth="40.0" layoutX="7.0" layoutY="4.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@/images/user.png" />
               </image>
            </ImageView>
            <Label fx:id="headOfDepNameLbl" layoutX="47.0" layoutY="14.0" prefHeight="20.0" prefWidth="151.0">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Button fx:id="btnStudent" layoutX="110.0" layoutY="190.0" mnemonicParsing="false" onAction="#getStudentStats" prefHeight="25.0" prefWidth="184.0" style="-fx-background-color: #D3B291;" text="View By Student" textFill="#344955" />
         </children>
      </Pane>
   </children>
</AnchorPane>
